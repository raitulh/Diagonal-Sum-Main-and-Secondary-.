#include <iostream>
using namespace std;

int main() {
    int n = 3;
    int arr[n][n];
    int mainSum = 0, secSum = 0;

    for(int i = 0; i < n; i++)
        for(int j = 0; j < n; j++)
            cin >> arr[i][j];

    for(int i = 0; i < n; i++) {
        mainSum += arr[i][i];
        secSum += arr[i][n - i - 1];
    }

    cout << "Main diagonal sum = " << mainSum << endl;
    cout << "Secondary diagonal sum = " << secSum << endl;

    return 0;
}
